<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="estate_property_form_view" model="ir.ui.view">
            <field name="name">estate.property.form</field>
            <field name="model">estate.property</field>
            <field name="arch" type="xml">
                <form string="Property">
                    <sheet>
                        <div class="oe_title">
                            <label for="name"/>
                            <h1>
                                <field name="name" placeholder="Title"/>
                            </h1>
                        </div>
                        <group>
                            <group>
                                <field name="create_date"/>
                            </group>
                            <group>
                                <field name="example_field"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="estate_property_tree_view" model="ir.ui.view">
            <field name="name">estate.property.tree</field>
            <field name="model">estate.property</field>
            <field name="arch" type="xml">
                <tree string="ModelTitle">
                    <field name="create_date"/>
                    <field name="example_field"/>
                </tree>
            </field>
        </record>





        <record id="estate_property_action" model="ir.actions.act_window">
            <field name="name">EstateProperty</field>
            <field name="res_model">estate.property</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="context">{"search_default_example_field":1}</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    There is no examples click here to add new real estate property.
                </p>
            </field>
        </record>

        <menuitem name="Estate property" id="EstateProperty_menu" parent="your_root_menu" action="estate_property_action"/>

    </data>
</odoo>